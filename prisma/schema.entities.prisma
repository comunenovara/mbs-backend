model Asset {
	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()
	description		String
	address		String
	mq		Float
}

model Dossier {
	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()
	description		String
}

model DossierType {
	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()
	description		String
	category		String
}

model Operation {
	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()
	description		String
	value		Decimal
	startDate		DateTime
	endDate		DateTime
}

model OperationType {
	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()
	description		String
}

model Relif {
	id          	Int      @id @default(autoincrement())
	createdAt   	DateTime @default(now())
	updatedAt   	DateTime @updatedAt()
	date		DateTime
	reason		String
}

